@using Radyn.Gallery.DataStructure
@model Gallery
@{
    ViewBag.Title = ViewBag.Category;
    Layout = "~/Views/Shared/User_Layout.cshtml";
}
@section Head{
}

<div class="col-lg-12">
    <div class="container-hd container-homa">
        <div class="container-header">
            <div class="container-header-item">
                <span>@ViewBag.Category</span>

            </div>
        </div>
        <div class="container-box" style="display: block;">




           <div id="myGallery" class="spacegallery">
                    @{
                        Html.RenderAction("GetNewImageSlider", "Photo", new { galleryId = Model.Id, area = "Gallery" });
                    }
                </div>
        </div>
    </div>
</div>
