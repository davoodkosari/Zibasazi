@using System.Web.Mvc.Html
@using Radyn.FormGenerator.DataStructure
@using Radyn.Web.Mvc;
@using Radyn.WebApp.Areas.FormGenerator.Tools
@model Radyn.FormGenerator.DataStructure.FormStructure
@{
    ViewBag.Title = Model!=null? Model.Name:string.Empty;
    Layout = "~/Views/Shared/User_Layout.cshtml";
}
@section Head{
  
}
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    if (Model != null)
    {
        <div class="form-generator-created">
            @{Html.FormSaveDataGenerator(Model.Id, new FormStructure().GetType().Name);}
        </div>
    }
   


}
