@using Radyn.Web.Mvc;
@using Radyn.WebApp.Areas.Common.Component
@model Radyn.Congress.DataStructure.Homa

@{
    ViewBag.Title = Resources.Common.Detail;
    Layout = "~/Views/Shared/_Layout.cshtml";

}
@section Head{}
@section PageTitle
{
    @Resources.Common.Detail

}
@section PageAction
{
<div class="button-div m-btn waves-red i-return">
    @{Html.Radyn().ImageActionLink().NavigateUrl(Url.Content("~/Congress/Homa/Index")).Text(Resources.Common.Return).Align(ImageActionLink.Alignment.Left).ImageUrl(Url.Content("~/Content/Images/go-back.png")).ImagePosition(ImageActionLink.Position.Right).Render();}
       </div>
}
@section PageActionDown
{
<div class="button-div m-btn waves-red i-return">
    @{Html.Radyn().ImageActionLink().NavigateUrl(Url.Content("~/Congress/Homa/Index")).Text(Resources.Common.Return).Align(ImageActionLink.Alignment.Left).ImageUrl(Url.Content("~/Content/Images/go-back.png")).ImagePosition(ImageActionLink.Position.Right).Render();}
       </div>

}
@using (Html.BeginForm())
{

    Html.RenderAction("GetDetails", "Homa", new { area = "Congress", Id = ViewBag.Id, });


}
