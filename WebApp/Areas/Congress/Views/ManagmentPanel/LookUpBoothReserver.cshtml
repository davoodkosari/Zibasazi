@using Radyn.Congress.DataStructure
@using Radyn.FormGenerator.Tools
@using Radyn.Web.Mvc
@using Radyn.WebApp.Areas.Congress.Tools
@using Radyn.WebApp.Areas.FormGenerator.Tools
@model Radyn.Congress.DataStructure.UserBooth
@{
    ViewBag.Title = Resources.Congress.BoothRezerv;
    Layout = "~/Views/Shared/LookUpLayout.cshtml";

}
@section Head{

    <script type="text/javascript">

        $(document).ready(function () {
            $("#DivItem").html("");
            $.get("@Url.Content("~/EnterpriseNode/EnterPriseNode/GenerateEnterpriseNodeModify")", { state: "Details", id: '@ViewBag.userId', date: new Date().getTime() }, function (data) {
                $("#DivItem").html(data).slideDown("slow");;
            });

        });

    </script>
}
@section PageAction
{
    <div class="button-div m-btn waves-red i-return">
        @{Html.Radyn().ImageActionLink().Align(ImageActionLink.Alignment.Left).ImageUrl(Url.Content("~/Content/Images/go-back.png")).ImagePosition(ImageActionLink.Position.Right).Text(@Resources.Common.Return).OtherAttribute(new { onclick = "Close_thisModal()" }).Render(); }
    </div>

}
@section PageActionDown
{
    <div class="button-div m-btn waves-red i-return">
        @{Html.Radyn().ImageActionLink().Align(ImageActionLink.Alignment.Left).ImageUrl(Url.Content("~/Content/Images/go-back.png")).ImagePosition(ImageActionLink.Position.Right).Text(@Resources.Common.Return).OtherAttribute(new { onclick = "Close_thisModal()" }).Render(); }
    </div>
}

<div id="DivItem"></div>
@{

    Html.FormGenerator(AppExtention.CongressMoudelName + "-/Congress/Booth/Booth", new UserBooth().GetType().Name, (Guid)ViewBag.userId + "," + (Guid)ViewBag.boothId, FormState.DetailsMode);

}
