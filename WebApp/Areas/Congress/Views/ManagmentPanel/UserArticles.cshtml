@using System.Web.Mvc.Html
@using Radyn.Web.Mvc;
@using Radyn.WebApp.AppCode.Security
@using Radyn.WebApp.Areas.Congress.Tools
@using Radyn.WebApp.Areas.FormGenerator.Tools
@using System.Web.Mvc
@model System.Collections.Generic.IEnumerable<Radyn.Congress.DataStructure.Article>
    @{
        ViewBag.Title = @Resources.Common.List + " " + string.Format(Resources.Congress.CongressArticle, SessionParameters.CurrentCongress.Configuration.ArticleTitle);
        Layout = "~/Views/Shared/_Layout.cshtml";

    }

    @section Head{


        <script type="text/javascript">
            function UpdateTotalStatus() {
                var con = confirm('آیا از تغییر وضعیت گروهی اطمینان دارید؟');
                if (con) {
                    ShowPostBackLoader();

                    var list = [];
                    var status = $('#finalstatus').val();
                    $('input[name="check-status"]:checked').each(function() {
                        var id = $(this).attr('id');
                        list.push(id);
                        var res = list.join(',');
                        $('#TotalStatusResult').val(res);
                    });
                    var result = $('#TotalStatusResult').val();
                    RadynPostData('/Congress/ManagmentPanel/ChangeArtilceStatusAsGroup',
                        { articles: result, status: status },
                        function() {
                            SearchAricle();
                        });
                } else {
                    return;
                }


            }


            function PrintList() {
                ShowPostBackLoader();
                RadynPostForm("@Url.Content("/Congress/ReportPanel/PrintArticlesList")",
                    "@Url.Content("~/Congress/ManagmentPanel/UserArticles")",
                    function(data) {
                        if (data != "false") {
                             window.open('@Url.Content("~/ReportBuilder/ReportView")');
                        }
                    });
            }

            function DesginArticlesReport() {
                ShowPostBackLoader();
                RadynPostForm("@Url.Content("~/Congress/ReportPanel/DesginArticlesReport")",
                   "@Url.Content("~/Congress/ManagmentPanel/UserArticles")",
                    function(data) {
                        if (data.Result != false) {
                            window.open(
                                "@Url.Content("~/ReportBuilder/ReportDesign?DataMethod=PreaperReport")&DataMethodParamets=" +
                                data.ReportId +
                                "&UpdateMethod=UpdateReport&UpdateParametrs=" +
                                data.ReportId);
                        }
                        ShowRadynMessage();
                    });

            }

            function Updatearticles() {
                ShowPostBackLoader();
                RadynPostForm("@Url.Content("~/Congress/ManagmentPanel/UpdateUserArticles")",
                   "@Url.Content("~/Congress/ManagmentPanel/UserArticles")",
                    function(data) {
                        if (data != "false") {
                            ShowRadynMessage();
                            SearchAricle();
                        }
                        ShowRadynMessage();
                    });

            }

            function DeleteAricle(Id) {

                if (confirm('آیا از فرستادن رکورد مورد نظر به بخش آرشیو مطمئن هستید؟')) {
                    $.get("@Url.Content("~/Congress/ManagmentPanel/DeleteArticleData")",
                        { articleId: Id, date: new Date().getTime() },
                        function(data) {
                            if (data != "") {
                                SearchAricle();
                            }
                            ShowRadynMessage();
                            //SearchAricle();
                        });
                    ShowRadynMessage();
                }
            }

            function SearchAricle() {

                ShowPostBackLoader();
                $("#DivArtileResult").html("");
                RadynPostForm("@Url.Content("~/Congress/ManagmentPanel/UserArticles")",
                   "@Url.Content("~/Congress/ManagmentPanel/UserArticles")",
                    function(data) {

                        if (data != "false") {
                            $("#DivArtileResult").html(data);
                            $("#DivArtileResult").slideDown();
                        }


                    });

            }

            function DesginAbstractArticle() {
                ShowPostBackLoader();
                $.get("@Url.Content("~/Congress/ReportPanel/DesginAbstractArticle")",
                    { date: new Date().getTime() },
                    function(data) {
                        if (data.Result != false) {
                            window.open(
                                "@Url.Content("~/ReportBuilder/ReportDesign?DataMethod=PreaperReport")&DataMethodParamets=" +
                                data.ReportId +
                                "&UpdateMethod=UpdateReport&UpdateParametrs=" +
                                data.ReportId);
                        }
                        ShowRadynMessage();
                    }).complete(function() {
                    HidePostBackLoader();
                });
            }

            function PrintCertificate(Id) {
                window.open('@Url.Content("~/Congress/ReportPanel/PrintArticleCertificate?Id=")' +
                    Id +
                    "&Isadmin=true");
                ShowRadynMessage();
            }

            function PrintArticlesAbstract() {
                ShowPostBackLoader();
                $.post("@Url.Content("~/Congress/ReportPanel/PrintArticlesAbstract")",
                    $('form[action^="@Url.Content("~/Congress/ManagmentPanel/UserArticles")"]').serialize(),
                    function(data) {
                        if (data != "false") {
                             window.open('@Url.Content("~/ReportBuilder/ReportView")');
                        }
                        ShowRadynMessage();
                    }).complete(function() {
                    HidePostBackLoader();
                });
            }

            function DesginCertificate() {
                ShowPostBackLoader();
                $.get("@Url.Content("~/Congress/ReportPanel/DesginArticleCertificate")",
                    { date: new Date().getTime() },
                    function(data) {
                        if (data.Result != false) {
                            window.open(
                                "@Url.Content("~/ReportBuilder/ReportDesign?DataMethod=PreaperReport")&DataMethodParamets=" +
                                data.ReportId +
                                "&UpdateMethod=UpdateReport&UpdateParametrs=" +
                                data.ReportId);
                        }
                        ShowRadynMessage();
                    }).complete(function() {
                    HidePostBackLoader();
                });
            }

            function PrintCertificateList() {
                ShowPostBackLoader();
                $.post("@Url.Content("~/Congress/ReportPanel/PrintAllArticleCertificate")",
                    $('form[action^="@Url.Content("~/Congress/ManagmentPanel/UserArticles")"]').serialize(),
                    function(data) {
                        if (data != "false") {
                             window.open('@Url.Content("~/ReportBuilder/ReportView")');
                        }
                        ShowRadynMessage();
                    }).complete(function() {
                    HidePostBackLoader();
                });

            }

            function SaveRecord(Id) {
              
                var con = confirm("آیا مطمئن هستید ؟‌");
                if (con) {
                    ShowPostBackLoader();
                    var articleType = $('#drpArticleType-' + Id + ' option:selected').val();
                    var finalStatus = $('#drpfinalstatus-' + Id + ' option:selected').val();
                    $.post("@Url.Content("~/Congress/ManagmentPanel/UpdateRecord")",
                        { id: Id, articleType: articleType, finalStatus: finalStatus },
                        function(data) {
                            if (data != "false") {

                            }
                            ShowRadynMessage();
                        }).complete(function() {
                        HidePostBackLoader();
                    });
                }

            }


            function DownloadZipFile() {
                ShowPostBackLoader();
                $.post("@Url.Content("~/Congress/ReportPanel/DownloadAllarticleZipFile")",
                    $('form[action^="@Url.Content("~/Congress/ManagmentPanel/UserArticles")"]').serialize(),
                    function(data) {
                        window.open('@Url.Content("~/Congress/ReportPanel/DownloaArticleZipFile")');
                        ShowRadynMessage();
                    }).complete(function() {
                    HidePostBackLoader();
                });


            }


            $(document).ready(function() {
                SearchAricle();


            });

            function ChangePivotCategory() {
                var id = $('#PivotCategoryId').val();
                $('#Search_PivotId').html('');
                $('#Search_PivotId').append($('<option>').text("@Resources.Congress.All"));
                $.get('/Congress/ManagmentPanel/GetPivot',
                    { id: id },
                    function(data) {
                        for (var i = 0; i < data.length; i++) {
                            var op = $('<option>').attr('value', data[i].Id).text(data[i].Title);
                            $('#Search_PivotId').append(op);
                        }
                    })
            }
        </script>

    }
