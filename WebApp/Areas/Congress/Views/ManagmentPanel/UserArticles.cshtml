                        </div>
                    </div>
                    <div class="table-row-item">
                        <div class="item">
                            @Html.DropDownList("SearchStatus", new SelectList(ViewBag.status, "Key", "Value"), "" + Resources.Congress.All + "", new { })
                        </div>
                    </div>
                </div>



                <div class="item-row">
                    <div class="table-row-cap">
