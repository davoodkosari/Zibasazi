@using Radyn.Web.Mvc;
@model Radyn.Congress.DataStructure.Referee
@{
    ViewBag.Title = Resources.Common.Detail;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Head{}
@section PageTitle
{
    @Resources.Common.Detail

}
@section PageAction
{
<div class="button-div m-btn waves-red i-return">
    @{Html.Radyn().ImageActionLink().NavigateUrl(Url.Content("~/Congress/Referee/Index")).Text(Resources.Common.Return).Align(ImageActionLink.Alignment.Left).ImageUrl(Url.Content("~/Content/Images/go-back.png")).ImagePosition(ImageActionLink.Position.Right).Render();}


</div>
}
@section PageActionDown
{
<div class="button-div m-btn waves-red i-return">
    @{Html.Radyn().ImageActionLink().NavigateUrl(Url.Content("~/Congress/Referee/Index")).Text(Resources.Common.Return).Align(ImageActionLink.Alignment.Left).ImageUrl(Url.Content("~/Content/Images/go-back.png")).ImagePosition(ImageActionLink.Position.Right).Render();}


</div>

}
@Html.ValidationSummary(true)
@{
    Html.RenderAction("GetDetails", "Referee", new { area = "Congress", Id = ViewBag.Id, });
}



